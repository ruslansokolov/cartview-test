{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },
  "dependencies": {
    "decimal.js": "^10.2.0",
    "expo": "^33.0.0",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-web": "^0.11.4",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "babel-preset-expo": "^5.1.1",
    "detox": "^12.11.1",
    "detox-expo-helpers": "^0.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "expo-detox-hook": "^1.0.10",
    "jest": "^24.8.0",
    "jest-expo": "^33.0.2",
    "react-test-renderer": "^16.8.6",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "jest-expo",
    "testRegex": ".*\\.test\\.(tsx|ts)",
    "setupFilesAfterEnv": [
      "<rootDir>helpers/setupTests.js"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "jest"
  },
  "private": true
}
